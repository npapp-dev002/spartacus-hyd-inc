{
  "name": "cart-demo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/cart-demo/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/cart-demo",
        "index": "apps/cart-demo/src/index.html",
        "browser": "apps/cart-demo/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/cart-demo/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/cart-demo/public"
          }
        ],
        "styles": ["apps/cart-demo/src/styles.scss"],
        "scripts": [],
        "server": "apps/cart-demo/src/main.server.ts",
        "prerender": false,
        "ssr": {
          "entry": "apps/cart-demo/src/server.ts"
        }
      },
      "configurations": {
        "production": {
          "namedChunks": true,
          "optimization": false,
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        },
        "production-no-opt": {
          "outputHashing": "all",
          "optimization": false,
          "sourceMap": true
        },
        "noSsr": {
          "ssr": false,
          "prerender": false
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "hmr": false,
          "buildTarget": "cart-demo:build:production,noSsr"
        },
        "development": {
          "hmr": false,
          "buildTarget": "cart-demo:build:development,noSsr"
        }
      },
      "defaultConfiguration": "development"
    },
    "serve:ssr": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node dist/apps/cart-demo/server/server.mjs",
        "dependsOn": ["build"]
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "cart-demo:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/cart-demo/jest.config.ts"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "cart-demo:build",
        "port": 4200,
        "staticFilePath": "dist/apps/cart-demo/browser",
        "spa": true
      }
    }
  }
}
